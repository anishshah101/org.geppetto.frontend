            }

            window.USE_SSL = <%= useSsl %>;
        </script>
    /build -->

    <!-- build:template:dev
        <link rel="stylesheet/less" type="text/css" href="assets/less/main.less" />
        <script src="assets/js/vendor/less-1.7.5.min.js" type="text/javascript"></script>
