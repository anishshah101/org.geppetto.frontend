<tns:simulation xmlns:tns="http://www.openworm.org/simulationSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openworm.org/simulationSchema ../../src/main/resources/schema/simulationSchema.xsd ">
	<tns:configuration>
		<tns:outputFormat>$ENTER FORMAT</tns:outputFormat>
	</tns:configuration>
	<tns:entity>
		<tns:aspect>
			<tns:id>$ENTER ID</tns:id>
			<tns:modelInterpreter>$ENTER MODAL INTERPRETER</tns:modelInterpreter>
			<tns:modelURL>$ENTER MODEL URL</tns:modelURL>
			<tns:simulator>$ENTER SIMULATOR</tns:simulator>
		</tns:aspect>
	</tns:entity>
	<tns:name>$ENTER NAME</tns:name>
	<!-- Optional input for script-->
	<tns:script>$ENTER URL</tns:script>
</tns:simulation>